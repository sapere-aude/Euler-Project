
//Problem included the fact that 2520 was the smallest number that could be divided by 1-10
#include<iostream>

int main(){

	int num = 2520;

	while (num % 11 != 0 || num % 12 != 0 || num % 13 != 0 
		|| num % 14 != 0 || num % 15 != 0 || num % 16 != 0 
		|| num % 17 != 0 || num % 18 != 0 || num % 19 != 0 
		|| num % 20 != 0){
		num++;
	}

	std::cout << num;

	return 0;
}
